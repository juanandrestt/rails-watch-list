<nav class="navbar navbar-light bg-light">
  <a class="navbar-brand" href="#">Movies lists</a>
  <%= link_to "New list", new_list_path, class: "btn btn-primary" %>
</nav>

<div class="container">
  <h1>Lists</h1>
  <div id="lists" class=" bg-light">
    <% @lists.each do |list| %>
      <%= list.name %>
      <p>
        <%= link_to "See details", list, class: "btn btn-primary" %>
      </p>
    <% end %>
  </div>
</div>
